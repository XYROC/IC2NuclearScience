package nuclearscience.tile;

import net.minecraft.nbt.NBTTagCompound;
import net.minecraft.tileentity.TileEntity;
import nuclearscience.NuclearScience;
import nuclearscience.util.AdvancedPosition;

public class TileEntityTest extends TileEntity{
	
	@Override
	public void writeToNBT(NBTTagCompound nbt) {
		super.writeToNBT(nbt);
	}
	
	@Override
	public void readFromNBT(NBTTagCompound nbt) {
		super.readFromNBT(nbt);
	}
	
	public AdvancedPosition getPos() {
		NuclearScience.log(" "+worldObj+" | "+xCoord+ " | "+yCoord+" | "+zCoord);
		return new AdvancedPosition(worldObj, xCoord, yCoord, zCoord);
	}

}
